{
    "compilerOptions": {
      "target": "es5",
      "jsx": "preserve",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "incremental": true,
      "removeComments": true,
      "forceConsistentCasingInFileNames": true,
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "noEmit": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "baseUrl": "./",
      "paths": {
        "@/*": [
          "./*"
        ]
      },
      "module": "esnext"
    },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  